    "react-icons": "^3.9.0",
    "react-loader-spinner": "^3.1.5",
    "react-modal": "^3.11.2",
    "react-phone-input-2": "^2.12.1",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "4.3.1",
    "react-router-redux": "^4.0.8",
    "react-scripts": "3.4.0",
    "react-star-ratings": "^2.3.0",
    "react-toastify": "^5.5.0",
    "reactstrap": "7.1.0",
    "redux": "^4.0.5",
