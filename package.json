{
  "name": "granite-warehouse",
  "version": "1.0.0",
  "description": "Granite Warehouse Projects",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/granitewarehouse/granite-admin.git "
  },
  "keywords": [
    "granite",
    "granite_warehouse",
    "warehouse",
    "marble",
    "stone",
    "quartz",
    "porcelain",
    "backsplash"
  ],
  "author": "Creative Tim",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/granitewarehouse/granite-admin.git/issues"
  },
  "homepage": "./",
  "scripts": {
    "start": "set PORT=4000 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css --source-map true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "axios": "^0.19.0",
    "chart.js": "2.7.3",
    "ckeditor4-react": "^1.1.0",
    "classnames": "^2.2.6",
    "express": "^4.17.1",
    "history": "^4.10.1",
    "joi-browser": "^13.4.0",
    "moment": "2.24.0",
    "node-sass": "^4.13.1",
    "nouislider": "13.1.1",
    "react": "16.8.4",
    "react-chartjs-2": "2.7.4",
    "react-copy-to-clipboard": "5.0.1",
    "react-datepicker": "^2.14.0",
    "react-datetime": "2.16.3",
    "react-dom": "16.8.4",
